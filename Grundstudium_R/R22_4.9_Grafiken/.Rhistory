ggplot(airquality, aes(x = Temp, y = Ozone)) +
geom_point(
shape = 16,  # Filled points
color = "#FFA500",  # Orange color
alpha = 0.5,  # Semi-transparency (80% in original)
aes(size = Wind/5)  # Point size scaled by Wind/5
) +
scale_size_identity() +  # Use the actual size values from Wind/5
labs(
title = "Mehr Ozon an warmen Tagen",
x = "Temperatur [°Fahrenheit]",
y = "Ø Ozon [ppb]"
) +
coord_cartesian(xlim = c(50, 100)) +  # Set x-axis limits
theme_minimal() +
theme(
axis.text = element_text(angle = 0),  # Horizontal axis labels (las = 1)
legend.position = "none"  # Hide legend for size
)
