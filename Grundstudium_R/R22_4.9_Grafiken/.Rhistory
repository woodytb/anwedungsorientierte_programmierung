library(ggplot2)
ggplot(airquality, aes(x = Temp, y = Ozone)) +
geom_point(
shape = 16,
color = "#FFA500",
alpha = 0.5,
aes(size = Wind/5)  # Punktgröße nach Windstärke
) +
geom_smooth(method = "lm", color = "red", se = FALSE) +  # Trendlinie ohne Konfidenzintervall
labs(
title = "Zusammenhang zwischen Temperatur und Ozon",
subtitle = "Punktgröße = Windstärke (Wind/5)",
x = "Temperatur [°F]",
y = "Ozon-Konzentration [ppb]"
) +
scale_size_identity() +
theme_minimal() +
theme(legend.position = "none")
ggplot(airquality, aes(x = Temp, y = Ozone)) +
geom_density_2d_filled(alpha = 0.7) +  # Konturlinien mit Füllung
geom_point(color = "white", size = 1, alpha = 0.3) +  # Punkte zur Orientierung
labs(
title = "Verteilung von Ozon und Temperatur",
x = "Temperatur [°F]",
y = "Ozon [ppb]"
) +
theme_minimal()
# Temperatur in Gruppen einteilen (z. B. 10°F-Intervalle)
airquality$Temp_Group <- cut(airquality$Temp, breaks = seq(50, 100, by = 10))
ggplot(airquality, aes(x = Temp_Group, y = Ozone, fill = Temp_Group)) +
geom_boxplot(alpha = 0.7) +
labs(
title = "Ozon-Konzentration nach Temperaturgruppen",
x = "Temperaturgruppe [°F]",
y = "Ozon [ppb]"
) +
scale_fill_viridis_d() +  # Farbpalette
theme_minimal() +
theme(legend.position = "none")
ggplot(airquality, aes(x = Temp, y = Ozone)) +
